config global
	option enable '0'
	option port '7053'
	option onlyprimarydns '0'
	option ipv6usealternativedns '0'
	option alternativednsconcurrent '0'
	option fallback '0'
	option primaryippath '/etc/overture/ip_network_primary'
	option alternativeippath '/etc/overture/ip_network_alternative'
	option domainfinder '1'
	option primarydomainpath '/etc/overture/domain_primary'
	option alternativedomainpath '/etc/overture/domain_alternative'
	option hostfinder '0'
	option hostfilepath '/etc/overture/hosts'
	option minttl '0'
	option domainttlpath '/etc/overture/domain_ttl'
	option cachesize '0'
	option rejectqtype '255'